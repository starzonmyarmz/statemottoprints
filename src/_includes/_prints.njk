{% for state in states %}
  <div class="product">
    <a href="/products/prints/{{ state.name | slug }}">
      <img src="/images/product/{{ state.abbreviation }}_print_white.jpg" alt="Product shot of {{ state.name }}" loading="lazy">
      <h5>{{ state.name }}</h5>
      <div class="price">$12</div>
    </a>
  </div>
{% endfor %}
